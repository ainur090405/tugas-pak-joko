mixin CircleImage(src, alt, width, height)
  .circle-terpopuler
    img(src=src, alt=alt, width=width, height=height)

mixin CircleImageContainer(images)
  .circle-images-terpopuler
    .circle-container-terpopuler
      each image in images
        +CircleImage(image.src, image.alt, image.width, image.height)